PROCEDURE Duplicate(file1 : STRING, file2 : STRING)
  DECLARE BOOLEAN : STRING

  OPENFILE file1 FOR READ
  OPENFILE file2 FOR WRITE

  WHILE NOT EOF(file1)
    READFILE file1, line
    WRITEFILE file2, line
  ENDWHILE

  CLOSEFILE file2
  CLOSEFILE file1
ENDPROCEDURE

Duplicate("data/names.txt", "data/new.txt")